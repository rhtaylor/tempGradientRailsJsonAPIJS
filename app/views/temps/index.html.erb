<p id="notice"><%= notice %></p>

<h1>Temps</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Temp high</th>
      <th>Temp mid</th>
      <th>Temp low</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temps.each do |temp| %>
      <tr>
        <td><%= temp.date %></td>
        <td><%= temp.temp_high %></td>
        <td><%= temp.temp_mid %></td>
        <td><%= temp.temp_low %></td>
        <td><%= temp.city_id %></td>
        <td><%= link_to 'Show', temp %></td>
        <td><%= link_to 'Edit', edit_temp_path(temp) %></td>
        <td><%= link_to 'Destroy', temp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Temp', new_temp_path %>
